{
  "name": "joke-nestjs",
  "version": "1.0.0",
  "description": "JokeAPI NestJS implementation",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "test": "jest",
    "docker-build": "npm run build && docker build -t nestjs-joke:latest ."
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sql.js": "^1.8.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "sqlite3": "^5.1.6",
    "typescript": "^5.5.0",
    "ts-node": "^10.9.1",
    "jest": "^29.6.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.5.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0"
  }
}
